     "ciso8601==2.1.3",
@@ -43,13 +43,13 @@ REQUIRES = [
     "jinja2>=2.11.1",
     "PyJWT==1.7.1",
     # PyJWT has loose dependency. We want the latest one.
-    "cryptography==2.8",
+    "cryptography>=2.8",
     "pip>=8.0.3",
     "python-slugify==4.0.0",
     "pytz>=2019.03",
